{
  "ansible": {
    "fileMatch": [
      "(^|\/)(?:docker-)?compose[^\/]*\\.ya?ml(?:\\.jinja)$"
    ]
  },
  "enabledManagers": [
    "ansible",
    "docker-compose",
    "pre-commit"
  ],
  "docker-compose": {
    "fileMatch": [
      "(^|\/)(?:docker-)?compose[^\/]*\\.ya?ml(?:\\.jinja)$"
    ]
  },
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "includeForks": true,
  "labels": [
    "dependencies"
  ],
  "onboarding": false,
  "packageRules": [
    {
      "matchPackageNames": [
        "linuxserver/qbittorrent"
      ],
      "allowedVersions": "<=13.0.0"
    }
  ],
  "platform": "github",
  "pre-commit": {
    "enabled": true
  },
  "repositories": [
    "ferrarimarco/home-lab"
  ],
  "username": "renovate-release"
}
